{
  "start_urls": [
      "http://nypost.com",
      "https://nypost.com/sports/"
    ],
  "allowed_domains": [
      "nypost.com",
      "www.mypost.com"
    ],
  "node_list_selectors": [
      "div.headline-container",
      "div.circular-title",
      "li.trending-story",
      "article.oversize-headline",
      "div.entry-header"
    ],
  "link_extractor": "a ::attr(href)",
  "follow_links": true,
  "page_title": "//meta[@property='og:title']/@content",
  "page_desc": "//meta[@property='og:description']/@content",
  "modified_time": "//meta[@property='article:modified_time']/@content",
  "published_time": "//meta[@property='article:published_time']/@content"
}
